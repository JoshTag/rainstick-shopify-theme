.nectar_image_with_hotspots .nectar_hotspot_wrap {
    position: absolute!important;
    width: 30px;
    height: 30px;
}
.nectar_image_with_hotspots .nectar_hotspot{
border-radius: 100px;
    position: absolute;
    z-index: 100;
    cursor: pointer;
    text-align: center;
    color: #fff4ec;
    display: block;
    background-color: #09c6c6!important;height: 30px;
    width: 30px;
